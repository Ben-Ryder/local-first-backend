{
  "extends": [
    "@commitlint/config-conventional"
  ],
  "rules": {
    "type-enum": [2, "always", [
      "build",
      "chore",
      "ci",
      "deps",
      "docs",
      "feat",
      "fix",
      "refactor",
      "revert",
      "style",
      "test",
      "release",
      "wip"
    ]]
  }
}
